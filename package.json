{
  "name": "hackathon-starter",
  "version": "4.3.0",
  "description": "A boilerplate for Node.js web applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahat/hackathon-starter.git"
  },
  "author": "Sahat Yalkabov",
  "license": "MIT",
  "scripts": {
    "start": "webpack --config ./webpack.config.prod.js && cross-env NODE_ENV=production babel-node server.js",
    "dev": "cross-env NODE_ENV=development babel-node server.js",
    "build": "rimraf dist && webpack --config ./webpack.config.js",
    "lint": "eslint ./src",
    "test": "mocha --reporter spec",
    "postinstall": "npm rebuild node-sass"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "clockwork": "^0.1.4",
    "compression": "^1.7.0",
    "connect-mongo": "^1.3.2",
    "css-loader": "0.28.7",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "express-flash": "^0.0.2",
    "express-graphql": "0.6.11",
    "express-session": "^1.15.5",
    "express-status-monitor": "^1.0.0",
    "express-validator": "^3.2.1",
    "extract-text-webpack-plugin": "3.0.1",
    "fbgraph": "^1.4.1",
    "github": "^9.2.0",
    "graphql": "0.11.3",
    "history": "4.7.2",
    "instagram-node": "^0.5.8",
    "keen-js": "4.0.1",
    "keen-tracking": "1.2.0",
    "lastfm": "^0.9.2",
    "lob": "^4.1.1",
    "lusca": "^1.5.1",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "node-foursquare": "^0.3.0",
    "node-linkedin": "^0.5.6",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^4.0.1",
    "passport": "0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "passport-openid": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "paypal-rest-sdk": "^1.7.1",
    "postcss-loader": "2.0.6",
    "pug": "^2.0.0-rc.3",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router-dom": "4.2.2",
    "request": "^2.81.0",
    "sass-loader": "6.0.6",
    "sinon": "3.2.1",
    "stripe": "^4.24.0",
    "style-loader": "0.19.0",
    "tumblr.js": "^1.1.1",
    "twilio": "^3.6.3",
    "twit": "^2.2.9",
    "uuid": "3.1.0",
    "validator": "^8.0.0",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-preset-env": "1.6.0",
    "babel-preset-react-hmre": "1.1.1",
    "chai": "^4.1.1",
    "cross-env": "5.0.5",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "sinon": "^3.2.1",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    },
    "plugins": [
      "chai-friendly"
    ]
  }
}
